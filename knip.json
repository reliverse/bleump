{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": [
    "deprecated/**/*.ts",
    ".{config,tests}/**/*.ts",
    "**/*.{config,cfg}.{ts,js}",
    "src/mod.ts",
    "src/**/*-mod.ts"
  ],
  "ignore": [
    "node_modules",
    "dist-jsr",
    "dist-npm",
    "dist-libs",
    "src/types.ts"
  ],
  "ignoreDependencies": ["@reliverse/rse"]
}
